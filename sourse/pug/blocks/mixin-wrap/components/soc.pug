mixin item-sos(icon, a='#')
	+e.A.item(href= a target="_blank")
		if block
			block
		else
			+icon(icon)
//- social buttons
mixin soc()
	+b.soc
		if block
			block
		else
			+item-sos('instagram', 'https://www.instagram.com/zvenigorodsky_posad/')
			+item-sos('fb')
			+item-sos()
				span Ð’

